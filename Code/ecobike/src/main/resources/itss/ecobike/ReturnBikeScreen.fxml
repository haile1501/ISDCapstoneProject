<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="950" prefWidth="1300" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="itss.ecobike.views.ReturnBike">
    <children>
        <Pane prefHeight="102.0" prefWidth="1300.0" style="-fx-background-color: #00a13f;">
            <children>
                <ImageView fitHeight="100.0" fitWidth="100.0" layoutX="80.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@images/logo.png" />
                    </image>
                </ImageView>
                <FontAwesomeIconView fill="WHITE" glyphName="SIGN_IN" layoutX="1189.0" layoutY="62.0" size="30" />
            </children>
        </Pane>
        <Label layoutX="220.0" layoutY="198.0" text="Select a dock to return bike" fx:id="title">
            <font>
                <Font name="System Bold" size="25.0" />
            </font>
        </Label>
        <Pane fx:id="back" layoutX="210.0" layoutY="132.0" prefHeight="38.0" prefWidth="34.0">
            <children>
                <FontAwesomeIconView glyphName="ARROW_LEFT" layoutX="8.0" layoutY="26.0" size="20" />
            </children>
        </Pane>
        <ScrollPane layoutX="295.0" layoutY="277.0" prefHeight="540.0" prefWidth="610.0">
            <content>
                <VBox fx:id="docksContainer" prefHeight="0.0" prefWidth="710.0" />
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
